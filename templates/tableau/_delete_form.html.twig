<form method="post" action="{{ path('app_tableau_delete', {'id': tableau.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tableau.id) }}">
    <button class="btn btn-dark btn-sm">{{ button_label|default('Supprimer') }}</button>
</form>
